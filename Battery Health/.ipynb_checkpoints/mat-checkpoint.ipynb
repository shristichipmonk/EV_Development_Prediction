    "import pandas as pd\n",
    "mat = scipy.io.loadmat('path_to_/fiename.mat') \n",
    "mat = {k:v for k, v in mat.items() if k[0] != '_'}\n",
    "data = pd.DataFrame({k: pd.Series(v[0]) for k, v in mat.iteritems()})\n",
    "data.to_csv(\"fiename.csv\")\n"
